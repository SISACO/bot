(async () => {
	switch (command) {
		case "tts": {
			if (querie === "help") {
				await m.reply(`*â—Command:* Text-To-Speech\n*ğŸ€Aliases* -tts\n*ğŸ§©Category:* Downloader/Utils\n*ğŸ› ï¸Usage:* ${
                prefix + command
              } Text\n\n*ğŸ“šDescription:* Changes your text into Voice`);
				return;
			}
			let texttts = text ? text : m.quoted && m.quoted.text ? m.quoted.text : m.text;
			const googleTTS = require("google-tts-api"); // CommonJS
			const ttsurl = googleTTS.getAudioUrl(texttts, {
